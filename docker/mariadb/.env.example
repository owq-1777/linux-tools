# Example env for docker/mariadb/docker-compose.yaml
# Copy to .env and set strong passwords before starting.

# Optional: override image
MARIADB_IMAGE=mariadb:11

# Required: set a strong root password
MARIADB_ROOT_PASSWORD=ChangeThisRootPass

# Optional app database + user
MARIADB_DATABASE=appdb
MARIADB_USER=app
MARIADB_PASSWORD=app_pass

# Timezone
TZ=UTC
